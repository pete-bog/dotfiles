{
    "autoDocstring.docstringFormat": "google",
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingQuotes": "never",
    "editor.find.autoFindInSelection": "multiline",
    "editor.fontFamily": "'Meslo LG S', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontSize": 15,
    "editor.formatOnSave": true,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.size": "fill",
    "editor.rulers": [
        80
    ],
    "files.trimTrailingWhitespace": true,
    "gitProjectManager.baseProjectsFolders": [
        "/home/user/development/"
    ],
    "outline.showConstants": false,
    "outline.showEnumMembers": false,
    "outline.showModules": false,
    "outline.showVariables": false,
    "python.defaultInterpreterPath": "/usr/bin/python3",
    "python.formatting.provider": "yapf",
    "python.formatting.yapfArgs": [
        "--style=google"
    ],
    "python.jediEnabled": false,
    "python.linting.flake8Args": [
        "--max-line-length=80"
    ],
    "python.linting.pydocstyleArgs": [
        "--convention=google"
    ],
    "python.linting.pydocstyleEnabled": true,
    "python.linting.pylintArgs": [
        "--disable=invalid-name",
        "--disable=too-few-public-methods",
        "--disable=too-many-public-methods"
    ],
    "python.linting.pylintEnabled": true,
    "python.linting.pylintUseMinimalCheckers": false,
    "python.sortImports.path": "isort",
    "python.testing.autoTestDiscoverOnSaveEnabled": true,
    "python.testing.nosetestPath": "nose2",
    "python.testing.nosetestsEnabled": true,
    "python.workspaceSymbols.enabled": true,
    "terminal.integrated.fontSize": 15,
    "vim.insertModeKeyBindings": [
        {
            "after": [
                "<Esc>"
            ],
            "before": [
                "j",
                "j"
            ]
        }
    ],
    "vim.leader": ",",
    "vim.useCtrlKeys": false,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.startupEditor": "none",
    "terminal.integrated.rendererType": "dom",
    "editor.semanticHighlighting.enabled": false,
    "workbench.colorCustomizations": {
        "[Ace Palenight]": {
            "terminal.background": "#282c35",
            "terminal.foreground": "#bfc7d5",
            "terminal.ansiBlack": "#292D3E",
            "terminal.ansiRed": "#BE5046",
            "terminal.ansiGreen": "#C3E88D",
            "terminal.ansiYellow": "#F78C6C",
            "terminal.ansiBlue": "#82b1ff",
            "terminal.ansiMagenta": "#c792ea",
            "terminal.ansiCyan": "#89DDFF",
            "terminal.ansiWhite": "#bfc7d5",
            "terminal.ansiBrightBlack": "#292D3E",
            "terminal.ansiBrightRed": "#ff869a",
            "terminal.ansiBrightGreen": "#C3E88D",
            "terminal.ansiBrightYellow": "#ffcb6b",
            "terminal.ansiBrightBlue": "#82b1ff",
            "terminal.ansiBrightMagenta": "#c792ea",
            "terminal.ansiBrightCyan": "#89DDFF",
            "terminal.ansiBrightWhite": "#c8ccd4",
            "editor.foreground": "#bfc7d5"
        },
        "[Atom One Dark]": {
            "editorCursor.foreground": "#bfc7d5",
            "terminal.foreground": "#ABB2BF",
            "terminal.ansiBlack": "#282C34",
            "terminal.ansiRed": "#E06C75",
            "terminal.ansiGreen": "#98C379",
            "terminal.ansiYellow": "#D19A66",
            "terminal.ansiBlue": "#61AFEF",
            "terminal.ansiMagenta": "#C678DD",
            "terminal.ansiCyan": "#56B6C2",
            "terminal.ansiWhite": "#ABB2BF",
            "terminal.ansiBrightBlack": "#282C34",
            "terminal.ansiBrightRed": "#BE5046",
            "terminal.ansiBrightGreen": "#98C379",
            "terminal.ansiBrightYellow": "#E5C07B",
            "terminal.ansiBrightBlue": "#61AFEF",
            "terminal.ansiBrightMagenta": "#C678DD",
            "terminal.ansiBrightCyan": "#56B6C2",
            "terminal.ansiBrightWhite": "#ABB2BF"
        }
    },
    "editor.tokenColorCustomizations": {
        "[Ace Palenight]": {
            "textMateRules": [
                {
                    "scope": "meta.function-call.arguments.python",
                    "settings": {
                        "foreground": "#bfc7d5"
                    }
                },
                {
                    "scope": "constant.other",
                    "settings": {
                        "foreground": "#bfc7d5"
                    }
                },
            ]
        },
        "[Atom One Dark]": {
            "textMateRules": [
                {
                    "scope": "variable.language.special.self.python",
                    "settings": {
                        "foreground": "#ABB2BF"
                    }
                }
            ]
        }
    },
    "editor.occurrencesHighlight": false,
    "workbench.colorTheme": "Atom One Dark",
    "vim.overrideCopy": false
}